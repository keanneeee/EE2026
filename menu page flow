            else if (pixel_index == 3108 || pixel_index == 3148 || ((pixel_index >= 4465) && (pixel_index <= 4510)) || pixel_index == 4561 || pixel_index == 4606 || pixel_index == 4657 || pixel_index == 4702 || pixel_index == 4753 || pixel_index == 4798 || pixel_index == 4849 || pixel_index == 4894 || pixel_index == 4945 || pixel_index == 4990 || pixel_index == 5041 || pixel_index == 5086 || pixel_index == 5137 || pixel_index == 5182 || pixel_index == 5233 || pixel_index == 5278 || pixel_index == 5329 || pixel_index == 5374 || pixel_index == 5425 || pixel_index == 5470 || pixel_index == 5521 || pixel_index == 5566 || pixel_index == 5617 || pixel_index == 5662 || pixel_index == 5713 || pixel_index == 5758 || (pixel_index >= 5809) && (pixel_index <= 5854)) oled_data = 16'b1111011110011110;
            else oled_data = 0;

        end else if (count == 2) begin
            // oled data soccer field, How To Play, btnU - rotate anticlockwise, next selected
        end else if (count == 3) begin
            // oled data soccer field, logo, 2P, 1P, back w 2P selected
        end else if (count == 4) begin
            // oled data soccer field, logo, 2P, 1P, back w 1P selected        
        end else if (count == 5) begin
            // oled data soccer field, logo, 2P, 1P, back w back selected    
        end else if (count == 6) begin
            // oled data w soccer field, kick off, back w kick off selected        
        end else if (count == 7) begin
            // oled data w soccer field, kick off, back w back selected      
        end else if (count == 8) begin
            oled_data <= 16'b0000000000000001; // actual game play w me and keanes code so dont display anything        
        end else if (count == 9) begin
            // oled data soccer field, logo, Player 1 wins, press btnC to play again, w btnC selected
        end else if (count == 10) begin
            // oled data soccer field, logo, Player 2 wins, press btnC to play again, w btnC selected     
        end else if (count == 11) begin
            // oled data soccer field, How To Play, btnD - rotate clockwise, next selected
        end else if (count == 12) begin
            // oled data soccer field, How To Play, btnC - Press and hold to shoot!, back selected
        end 
    end 
    
endmodule
