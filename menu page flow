`timescale 1ns / 1ps

module menu(input [2:0] game_set, input [12:0] pixel_index, input btnR, btnL, btnC, btnU, btnD, input basys_clock, output reg [15:0] oled_data, output reg [4:0] count = 0);
    
    // game_set = 0 if still playing, game_set = 1 if P1 wins, game_set = 2 if P2_wins, game_set = 3 if score is 2-2
    // count = 0 oled data soccer field, logo, play, controls w play selected
    // count = 1 oled data soccer field, logo, play, controls w controls selected
    // count = 2 oled data soccer field, How To Play, btnU - rotate anticlockwise, next selected
    // count = 3 oled data soccer field, logo, 2P, 1P, back w 2P selected
    // count = 4 oled data soccer field, logo, 2P, 1P, back w 1P selected     
    // count = 5 oled data soccer field, logo, 2P, 1P, back w back selected  
    // count = 6 oled data w soccer field, kick off, back w kick off selected        
    // count = 7 oled data w soccer field, kick off, back w back selected     
    // count = 8 actual game play w me and keanes code so dont display anything        
    // count = 9 oled data soccer field, logo, Player 1 wins, press btnC to play again, w btnC selected
    // count = 10 oled data soccer field, logo, Player 2 wins, press btnC to play again, w btnC selected
    // count = 11 oled data soccer field, How To Play, btnD - rotate clockwise, next selected
    // count = 12 oled data soccer field, How To Play, btnC - Press and hold to shoot!, back selected
    // count = 13 oled data for GOLDEN GOAL background is black
 
    wire clk_25M, clk_1hz;
    wire btnC_d, btnL_d, btnR_d, btnD_d, btnU_d;
    reg prev_btnC_d = 0, prev_btnL_d = 0, prev_btnR_d = 0;
    debounce unit_C(basys_clock, btnC, btnC_d);
    debounce unit_L(basys_clock, btnL, btnL_d);
    debounce unit_R(basys_clock, btnR, btnR_d);
    debounce unit_U(basys_clock, btnU, btnU_d);
    debounce unit_D(basys_clock, btnD, btnD_d);
    
    flexible_clock unit25Mhz (basys_clock, 1, clk_25M);
    flexible_clock unit1hz (basys_clock, 49_999_999, clk_1hz);
    
    // Loop to check state
    always @(posedge basys_clock) begin
        
        if (btnL_d && btnR_d && btnU_d && btnD_d) begin // reset count
                count <= 0;
        end else begin
            if (count == 0 && btnR_d && !prev_btnR_d) // main menu play selected -> main menu controls selected
                count <= 1;
            else if (count == 1 && btnL_d && !prev_btnL_d) // main menu controls selected -> main menu play selected
                count <= 0;
            else if (count == 1 && btnC_d && !prev_btnC_d) // main menu controls selected -> control page btnU w next selected
                count <= 2;
            else if (count == 2 && btnC_d && !prev_btnC_d) // control page btnU w next selected -> control page btnD w next selected
                count <= 11;
            else if (count == 11 && btnC_d && !prev_btnC_d) // control page btnD w next selected -> control page btnC w back selected
                count <= 12;
            else if (count == 12 && btnC_d && !prev_btnC_d) // control page btnC w back selected -> main menu controls selected
                count <= 1;
//            else if (count == 0 && btnC_d && !prev_btnC_d) // main menu play selected -> 1P selected
//                count <= 4;
            else if (count == 0 && btnC_d && !prev_btnC_d) // main menu play selected -> 2P selected
                count <= 3;
//            else if (count == 4 && btnR_d && !prev_btnR_d) // 1P selected -> 2P selected
//                count <= 3;
//            else if (count == 3 && btnL_d && !prev_btnL_d) // 2P selected -> 1P selected
//                count <= 4;
            else if (count == 3 && btnR_d && !prev_btnR_d) // 2P selected -> back selected
                count <= 5;
            else if (count == 5 && btnL_d && !prev_btnL_d) // back selected -> 2P selected
                count <= 3;
            else if (count == 5 && btnC_d && !prev_btnC_d) // back selected -> play selected
                count <= 0;
            else if (count == 3 && btnC_d && !prev_btnC_d) // 2P selected -> kick off selected
                count <= 6;
            else if (count == 6 && btnR_d && !prev_btnR_d) // kick off selected -> back selected
                count <= 7;
            else if (count == 7 && btnL_d && !prev_btnL_d) // back selected -> kick off selected
                count <= 6;
            else if (count == 7 && btnC_d && !prev_btnC_d) // back selected -> 2P selected
                count <= 3;
            else if (count == 6 && btnL && btnR) // kick off selected -> nothing
                count <= 8;
            else if (count == 8 && game_set == 0) // remain in normal game
                count <= 8;
            else if (count == 8 && game_set == 1) // nothing -> P1 wins, btnC selected
                count <= 9;
            else if  (count == 8 && game_set == 2) // nothing -> P2 wins, btnC selected
                count <= 10;
            else if (count == 9 && btnC_d && !prev_btnC_d) // P1 wins, btnC selected -> play selected
                count <= 0;
            else if (count == 10 && btnC_d && !prev_btnC_d) // P2 wins, btnC selected -> play selected
                count <= 0;      
            else if (count == 8 && game_set == 3)
                count <= 13;
            else if (count == 13 && game_set == 0)
                count <= 8;
        end
        prev_btnC_d <= btnC_d;
        prev_btnL_d <= btnL_d;
        prev_btnR_d <= btnR_d;
    end
    
    // loop for oled_data
    always @ (posedge clk_25M) begin
        if (count == 0) begin
            // oled data soccer field, logo, play, controls w play selected
            if (pixel_index == 0 || ((pixel_index >= 2) && (pixel_index <= 46)) || ((pixel_index >= 49) && (pixel_index <= 93)) || ((pixel_index >= 95) && (pixel_index <= 96)) || ((pixel_index >= 98) && (pixel_index <= 142)) || ((pixel_index >= 145) && (pixel_index <= 189)) || ((pixel_index >= 191) && (pixel_index <= 192)) || ((pixel_index >= 194) && (pixel_index <= 238)) || ((pixel_index >= 241) && (pixel_index <= 285)) || ((pixel_index >= 287) && (pixel_index <= 288)) || ((pixel_index >= 290) && (pixel_index <= 334)) || ((pixel_index >= 337) && (pixel_index <= 381)) || ((pixel_index >= 383) && (pixel_index <= 384)) || ((pixel_index >= 386) && (pixel_index <= 430)) || ((pixel_index >= 433) && (pixel_index <= 477)) || ((pixel_index >= 479) && (pixel_index <= 480)) || ((pixel_index >= 482) && (pixel_index <= 526)) || ((pixel_index >= 529) && (pixel_index <= 573)) || ((pixel_index >= 575) && (pixel_index <= 576)) || ((pixel_index >= 578) && (pixel_index <= 622)) || ((pixel_index >= 625) && (pixel_index <= 669)) || ((pixel_index >= 671) && (pixel_index <= 672)) || ((pixel_index >= 674) && (pixel_index <= 700)) || ((pixel_index >= 707) && (pixel_index <= 709)) || ((pixel_index >= 714) && (pixel_index <= 715)) || ((pixel_index >= 729) && (pixel_index <= 734)) || ((pixel_index >= 738) && (pixel_index <= 765)) || ((pixel_index >= 767) && (pixel_index <= 768)) || ((pixel_index >= 770) && (pixel_index <= 796)) || ((pixel_index >= 804) && (pixel_index <= 805)) || ((pixel_index >= 810) && (pixel_index <= 811)) || pixel_index == 816 || ((pixel_index >= 825) && (pixel_index <= 830)) || ((pixel_index >= 835) && (pixel_index <= 861)) || ((pixel_index >= 863) && (pixel_index <= 864)) || ((pixel_index >= 866) && (pixel_index <= 890)) || pixel_index == 901 || pixel_index == 907 || ((pixel_index >= 923) && (pixel_index <= 924)) || ((pixel_index >= 932) && (pixel_index <= 957)) || ((pixel_index >= 959) && (pixel_index <= 960)) || ((pixel_index >= 962) && (pixel_index <= 986)) || pixel_index == 1003 || ((pixel_index >= 1019) && (pixel_index <= 1020)) || ((pixel_index >= 1029) && (pixel_index <= 1053)) || ((pixel_index >= 1055) && (pixel_index <= 1056)) || ((pixel_index >= 1058) && (pixel_index <= 1082)) || pixel_index == 1099 || pixel_index == 1116 || ((pixel_index >= 1125) && (pixel_index <= 1149)) || ((pixel_index >= 1151) && (pixel_index <= 1152)) || ((pixel_index >= 1154) && (pixel_index <= 1178)) || pixel_index == 1195 || pixel_index == 1212 || ((pixel_index >= 1221) && (pixel_index <= 1245)) || ((pixel_index >= 1247) && (pixel_index <= 1248)) || ((pixel_index >= 1263) && (pixel_index <= 1274)) || pixel_index == 1291 || ((pixel_index >= 1318) && (pixel_index <= 1328)) || ((pixel_index >= 1343) && (pixel_index <= 1344)) || ((pixel_index >= 1359) && (pixel_index <= 1371)) || pixel_index == 1387 || ((pixel_index >= 1414) && (pixel_index <= 1424)) || ((pixel_index >= 1439) && (pixel_index <= 1440)) || ((pixel_index >= 1443) && (pixel_index <= 1453)) || ((pixel_index >= 1455) && (pixel_index <= 1467)) || pixel_index == 1477 || pixel_index == 1483 || ((pixel_index >= 1511) && (pixel_index <= 1520)) || ((pixel_index >= 1522) && (pixel_index <= 1533)) || ((pixel_index >= 1535) && (pixel_index <= 1536)) || ((pixel_index >= 1539) && (pixel_index <= 1549)) || ((pixel_index >= 1551) && (pixel_index <= 1562)) || pixel_index == 1579 || ((pixel_index >= 1607) && (pixel_index <= 1616)) || ((pixel_index >= 1618) && (pixel_index <= 1629)) || ((pixel_index >= 1631) && (pixel_index <= 1632)) || ((pixel_index >= 1634) && (pixel_index <= 1645)) || ((pixel_index >= 1647) && (pixel_index <= 1658)) || pixel_index == 1675 || ((pixel_index >= 1690) && (pixel_index <= 1691)) || ((pixel_index >= 1703) && (pixel_index <= 1712)) || ((pixel_index >= 1714) && (pixel_index <= 1725)) || ((pixel_index >= 1727) && (pixel_index <= 1728)) || ((pixel_index >= 1730) && (pixel_index <= 1741)) || ((pixel_index >= 1743) && (pixel_index <= 1754)) || ((pixel_index >= 1783) && (pixel_inde