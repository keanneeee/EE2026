module menu(input [2:0] game_set, input [12:0] pixel_index, input btnR, btnL, btnC, input basys_clock, output reg [15:0] oled_data, output reg [4:0] count = 0);
    
    // game_set = 0 if still playing, game_set = 1 if P1 wins, game_set = 2 if P2_wins
    
    wire clk_25M;
    
    flexible_clock unit25Mhz (basys_clock, 1, clk_25M);
    
    // loop to check state
    always @ (posedge basys_clock) begin
        if (count == 0 && btnR) // main menu play selected -> main menu controls selected
            count <= 1;
        else if (count == 1 && btnL) // main menu controls selected -> main menu play selected
            count <= 0;
        else if (count == 1 && btnC) // main menu controls selected -> control page btnU w next selected
            count <= 2;
        else if (count == 2 && btnC) // control page btnU w next selected -> control page btnD w next selected
            count <= 11;
        else if (count == 11 && btnC) // control page btnD w next selected -> control page btnC w back selected
            count <= 12;
        else if (count == 12 && btnC) // control page btnC w back selected -> main menu controls selected
            count <= 1;
        else if (count == 0 && btnC) // main menu play selected -> 2P selected
            count <= 3;
        else if (count == 3 && btnR) // 2P selected -> 1P selected
            count <= 4;
        else if (count == 4 && btnL) // 1P selected -> 2P selected
            count <= 3;
        else if (count == 4 && btnR) // 1P selected -> back selected
            count <= 5;
        else if (count == 5 && btnL) // back selected -> 1P selected
            count <= 4;
        else if (count == 5 && btnC) // back selected -> play selected
            count <= 0;
        else if (count == 3 && btnC) // 2P selected -> kick off selected
            count <= 6;
        else if (count == 6 && btnR) // kick off selected -> back selected
            count <= 7;
        else if (count == 7 && btnL) // back selected -> kick off selected
            count <= 6;
        else if (count == 7 && btnC) // back selected -> 2P selected
            count <= 3;
        else if (count == 6 && btnL && btnR) // kick off selected -> nothing
            count <= 8;
        else if (count == 8 && game_set == 1) // nothing -> P1 wins, btnC selected
            count <= 9;
        else if  (count == 8 && game_set == 2) // nothing -> P2 wins, btnC selected
            count <= 10;
        else if (count == 9 && btnC) // P1 wins, btnC selected -> play selected
            count <= 0;
        else if (count == 10 && btnC) // P2 wins, btnC selected -> play selected
            count <= 0;           
    end
    
    // loop for oled_data
    always @ (posedge clk_25M) begin
        if (count == 0) begin
            // oled data soccer field, logo, play, controls w play selected
            if (pixel_index == 0 || ((pixel_index >= 2) && (pixel_index <= 46)) || ((pixel_index >= 49) && (pixel_index <= 93)) || ((pixel_index >= 95) && (pixel_index <= 96)) || ((pixel_index >= 98) && (pixel_index <= 142)) || ((pixel_index >= 145) && (pixel_index <= 189)) || ((pixel_index >= 191) && (pixel_index <= 192)) || ((pixel_index >= 194) && (pixel_index <= 238)) || ((pixel_index >= 241) && (pixel_index <= 285)) || ((pixel_index >= 287) && (pixel_index <= 288)) || ((pixel_index >= 290) && (pixel_index <= 334)) || ((pixel_index >= 337) && (pixel_index <= 381)) || ((pixel_index >= 383) && (pixel_index <= 384)) || ((pixel_index >= 386) && (pixel_index <= 430)) || ((pixel_index >= 433) && (pixel_index <= 477)) || ((pixel_index >= 479) && (pixel_index <= 480)) || ((pixel_index >= 482) && (pixel_index <= 526)) || ((pixel_index >= 529) && (pixel_index <= 573)) || ((pixel_index >= 575) && (pixel_index <= 576)) || ((pixel_index >= 578) && (pixel_index <= 622)) || ((pixel_index >= 625) && (pixel_index <= 669)) || ((pixel_index >= 671) && (pixel_index <= 672)) || ((pixel_index >= 674) && (pixel_index <= 700)) || ((pixel_index >= 707) && (pixel_index <= 709)) || ((pixel_index >= 714) && (pixel_index <= 715)) || ((pixel_index >= 729) && (pixel_index <= 734)) || ((pixel_index >= 738) && (pixel_index <= 765)) || ((pixel_index >= 767) && (pixel_index <= 768)) || ((pixel_index >= 770) && (pixel_index <= 796)) || ((pixel_index >= 804) && (pixel_index <= 805)) || ((pixel_index >= 810) && (pixel_index <= 811)) || pixel_index == 816 || ((pixel_index >= 825) && (pixel_index <= 830)) || ((pixel_index >= 835) && (pixel_index <= 861)) || ((pixel_index >= 863) && (pixel_index <= 864)) || ((pixel_index >= 866) && (pixel_index <= 890)) || pixel_index == 901 || pixel_index == 907 || ((pixel_index >= 923) && (pixel_index <= 924)) || ((pixel_index >= 932) && (pixel_index <= 957)) || ((pixel_index >= 959) && (pixel_index <= 960)) || ((pixel_index >= 962) && (pixel_index <= 986)) || pixel_